@article{WU20103861,
abstract = {A computational fluid dynamics (CFD) model that simulates mechanical mixing for high-solids anaerobic digestion was developed. Numerical simulations of mixing manure slurry which exhibits non-Newtonian pseudo-plastic fluid behavior were performed for six designs: (i) one helical ribbon impeller; (ii) one anchor impeller; (iii) one curtain-type impeller; (iv) three counterflow (CF-2) impellers; (v) two modified high solidity (MHS 3/39°) impellers; and (vi) two pitched blade turbine impellers. The CFD model was validated against measurements for mixing a Herschel-Bulkley fluid by ribbon and anchor impellers. Based on mixing time with respect to mixing energy level, three impeller types (ribbon, CF-2, and MHS 3/39°) stand out when agitating highly viscous fluids, of these mixing with two MHS 3/39° impellers requires the lowest power input to homogenize the manure slurry. A comparison of digestion material demonstrates that the mixing energy varies with manure type and total solids concentration to obtain a given mixing time. Moreover, an in-depth discussion about the CFD strategy, the influences of flow regime and impeller type on mixing characteristics, and the intrinsic relation between mixing and flow field is included. {\textcopyright} 2012 Wiley Periodicals, Inc.},
author = {Terashima, Mitsuharu and Goel, Rajeev and Komatsu, Kazuya and Yasui, Hidenari and Takahashi, Hiroshi and Li, Y Y and Noike, Tatsuya and Rasi, S and Veijanen, A and Rintala, J and Madhania, Suci and Muharam, Yuswan and Winardi, Sugeng and Purwanto, Widodo Wahyu and Wu, Binxin and Wei, Peng and Mudde, Robert F and Uijttewaal, Wim and Spanjers, Henri and van Lier, Jules B and de Kreuk, Merle and Wu, Binxin and Dapelo, Davide and Alberini, Federico and Bridgeman, John and Yu, Zhao and Finch, Brian A. and Hale, Dean A. and Mata-Alvarez, J and Mac{\'{e}}, S and Llabr{\'{e}}s, P and Lindmark, Johan and Thorin, Eva and Fdhila, Rebei Bel and Dahlquist, Erik and Karim, Khursheed and Varma, Rajneesh and Vesvikar, Mehul and Al-Dahhan, M H and Weiland, Peter and Wu, Binxin},
doi = {https://doi.org/10.1016/j.watres.2018.10.077},
issn = {0043-1354},
journal = {Water Research},
keywords = {Anaerobic digester,Anaerobic digestion,Bioethanol,Biogas,Biogas plant,Biogasification,Biomethanization,CARPT,CFD,CFD modeling,CT,Co-digestion,Computational fluid dynamics,Continuously stirred tank reactor,Digester,Digestion enhancement,Euler–Lagrangian,Gas mixing,High-solids anaerobic digestion,Homogenization,Hydrodynamics,Hydrolysis,Industrial,Intermittently mixed,Landfill,Mechanical mixing,Methane,Mixing,Mixing behavior,Molasses–water,Multiphase flow,Non-Newtonian fluid,Numerical simulation,PIV,Power consumption,Pre-treatment,Rheology,Sewage treatment plant,Stirred-tank,Tracer,Turbulence model,Turbulent kinetic energy,Two-phase flow,Volatile organic compounds},
month = {jan},
